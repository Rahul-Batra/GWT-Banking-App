<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
		font-weight: bold;
		}
		.panel {
		margin-top:100px;
		width: 200px;
		height: 55px;
		margin-left: 300px;
		}
	</ui:style>
	<g:FlowPanel styleName="{style.panel}">
		<g:HTMLPanel>

			<div>
				primary holder name
				<g:TextBox ui:field="pholdername"></g:TextBox>
			</div>
			<div>
				address
				<g:TextArea ui:field="address"></g:TextArea>
			</div>
			<div>
				mobile
				<g:TextBox ui:field="mobile"></g:TextBox>
			</div>
			<div>
				emailid
				<g:TextBox ui:field="email"></g:TextBox>
			</div>
			<div>
				<g:Label ui:field="uservalid" styleName="(style.valid)"></g:Label>
			</div>
			<div>
				password
				<g:PasswordTextBox ui:field="passwd"></g:PasswordTextBox>
			</div>
			<div>
				Confirm Password
				<g:PasswordTextBox ui:field="cpswd"></g:PasswordTextBox>
			</div>
			<div>
				<g:Label ui:field="uservalid1" styleName="(style.valid)"></g:Label>
			</div>
			<div>
				pan number
				<g:TextBox ui:field="pan"></g:TextBox>
			</div>
			<div>
				<g:Label ui:field="uservalid2" styleName="(style.valid)"></g:Label>
			</div>
			<div>
				<g:Button ui:field="submitcust">Submit</g:Button>
				<g:Button ui:field="resetcust">Reset</g:Button>
			</div>

		</g:HTMLPanel>
	</g:FlowPanel>
</ui:UiBinder> 