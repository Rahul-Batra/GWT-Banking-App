<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.panel {
			width: 200px;
			height: 140px;
		}
	</ui:style>
	<g:FlowPanel styleName="{style.panel}" >
		<g:HTMLPanel>
			<div>
				Name :
				<g:TextBox ui:field="usernameTxt"></g:TextBox>
			</div>
			<div>
				Password :
				<g:PasswordTextBox ui:field="passwordTxt"></g:PasswordTextBox>
			</div>
			<div align="left">
				<g:Button ui:field="submitBttn">Login</g:Button>
			    <g:Button ui:field="resetBttn">Reset</g:Button>
		</div>
		
		
	</g:HTMLPanel>
		
	</g:FlowPanel>
</ui:UiBinder> 