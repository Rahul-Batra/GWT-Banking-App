<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.panel {
		margin-top:100px;
		width: 300px;
		height: 55px;
		margin-left: 100px;
		}
	</ui:style>
	<g:FlowPanel styleName="{style.panel}">
		<g:HTMLPanel>
				
			<div>
				Enter the Account no. transaction to be made:
				<g:TextBox ui:field="accnttran"></g:TextBox>
			</div>
			<div>
			Enter the Amount to be Credit or Debit:
				<g:TextBox ui:field="amnt"></g:TextBox>
			</div>
			
			<div>
				<g:Button ui:field="depst">Deposit</g:Button>
				<g:Button ui:field="withdrw">Withdraw</g:Button>		
				<g:Button ui:field="rest">Reset</g:Button>
			</div>

		</g:HTMLPanel>
	</g:FlowPanel>
</ui:UiBinder> 