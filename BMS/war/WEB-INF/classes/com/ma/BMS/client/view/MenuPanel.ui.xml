<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		
		.treebutton {
			font: 14px Arial, Sans-Serif;
	       padding:10px 5px 10px 5px;
	       margin:6px 12px 12px 50px;
	  	}
	
	.treeitem
	{
	margin:0px 0px 0px 5px;
	padding:0px;
	list-style-type:none;
	text-align:left;
	font-family:Arial,Helvetica,sans-serif;
	font-size:13px;
	font-weight:normal;
	cursor: pointer;
	}
	
	
	
	</ui:style>
	
	<g:HTMLPanel>
					
        <div class="vertical">
           
			<div id="left">
		    <div id="image">
			<g:Tree styleName="{style.treebutton}">
			   <g:TreeItem ui:field="SystemUser" text="SystemUser"
					styleName="{style.treeitem}">
					<g:InlineLabel text="add " ui:field="add" />
					<g:InlineLabel text="view " ui:field="viewsys" />
								
				</g:TreeItem>
	<g:TreeItem ui:field="customer" text="customer" styleName="{style.treeitem}">
					<g:InlineLabel text="add " ui:field="cusadd" />
					<g:InlineLabel text="modify " ui:field="modcus" />
					<g:InlineLabel text="view/delete" ui:field="viewcust" />
					<g:InlineLabel text="search" ui:field="searchcus"/>
				</g:TreeItem>
				<g:TreeItem ui:field="account" text="account" styleName="{style.treeitem}">
						<g:InlineLabel text="add " ui:field="accadd" />
						<g:InlineLabel text="search" ui:field="accsearch" />
						<g:InlineLabel text="view/delete" ui:field="accview" />
				</g:TreeItem>
				<g:InlineLabel ui:field="transaction" text="Transaction" styleName="{style.treeitem}">
				</g:InlineLabel>
				<g:InlineLabel ui:field="report" text="Generate report" styleName="{style.treeitem}">
			</g:InlineLabel>
			</g:Tree>
            </div>
            </div>
     					  			
		</div>
	     
				  
	</g:HTMLPanel>
</ui:UiBinder> 